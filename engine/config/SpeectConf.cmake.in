set(SPEECT_INCLUDE_PATH "@SPCT_INCLUDE_DIRS@")
set(SPEECT_VERSION_STRING @SPCT_VERSION@)
set(SPEECT_VERSION_MAJOR @SPCT_VERSION_MAJOR@)
set(SPEECT_VERSION_MINOR @SPCT_VERSION_MINOR@)
