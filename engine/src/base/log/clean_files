#!/bin/sh

echo "\t\t\tin log"


########################################################################

echo "\t\t\tcleaning event"
cd ./event/
./clean_files
cd ..


########################################################################

echo "\t\t\tcleaning layout"
cd ./layout/
./clean_files
cd ..


########################################################################

echo "\t\t\tcleaning streams"
cd ./streams/
./clean_files
cd ..


########################################################################


rm -f *~
